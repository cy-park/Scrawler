{
  "name": "scrawler",
  "version": "1.2.1",
  "description": "A vertical parallax library.",
  "main": "dist/Scrawler.js",
  "scripts": {
    "server": "browser-sync start --server '.' --files 'dist/*.js, examples/*.html' --host 'localhost' --port 8080 --open 'external'",
    "watch": "mkdir -p dist && onchange 'src/*.js' -- babel-node scripts/build.js --draft",
    "dist": "mkdir -p dist && babel-node scripts/build.js",
    "inst": "npm i babel-cli babel-preset-es2015 browser-sync concat js-beautify onchange uglify-js --save-dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cy-park/Scrawler.git"
  },
  "keywords": [
    "parallax",
    "scroll"
  ],
  "author": "Chan Young Park",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cy-park/Scrawler/issues"
  },
  "homepage": "https://github.com/cy-park/Scrawler#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.18.8",
    "concat": "^3.0.0",
    "js-beautify": "^1.6.12",
    "onchange": "^3.2.1",
    "uglify-js": "^2.8.22"
  },
  "dependencies": {},
  "metadata": {
    "class_name": "Scrawler",
    "year_launched": "2016"
  }
}
